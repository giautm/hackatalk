    "expo-image-picker": "^10.0.0",
    "expo-linear-gradient": "^9.0.0",
    "expo-localization": "^10.0.0",
    "expo-notifications": "^0.9.0",
    "expo-permissions": "^11.0.0",
    "expo-random": "^11.0.0",
    "expo-screen-orientation": "^3.0.0",
    "expo-splash-screen": "^0.9.0",
    "expo-updates": "^0.4.1",
    "expo-web-browser": "^9.0.0",
    "fbt": "^0.16.1",
    "i18n-js": "^3.8.0",
    "immer": "^8.0.1",
    "mime": "^2.5.0",
    "moment": "^2.29.1",
    "react": "^0.0.0-experimental-27659559e",
    "react-dom": "^0.0.0-experimental-27659559e",
    "react-native": "0.63.4",
    "react-native-dotenv": "^2.5.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-modalbox": "^2.0.2",
    "react-native-reanimated": "^2.0.0-rc.2",
    "react-native-redash": "^14.2.4",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-scalable-image": "^1.0.0",
    "react-native-screens": "^2.17.1",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-unimodules": "^0.12.0",
    "react-native-vector-icons": "^8.0.0",
    "react-native-web": "^0.14.10",
    "react-native-webview": "^11.2.1",
    "react-relay": "^0.0.0-experimental-4c4107dd",
    "react-responsive": "^8.2.0",
    "react-scripts": "^4.0.1",
